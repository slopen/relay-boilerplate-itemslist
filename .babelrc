{
	"presets": [
		"react",
		"es2015",
		"stage-0"
	],

	"env": {
		"development": {
			"plugins": [
				"react-hot-loader/babel",
				"transform-decorators",
				"react-relay",
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-catch-errors",
						"imports": [
							"react",
							"redbox-react"
						]
					}]
				}]
			]
		}
	}
}
